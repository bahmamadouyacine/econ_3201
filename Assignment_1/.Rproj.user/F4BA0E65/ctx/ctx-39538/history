𝑧1 = √
z_1 <- \sqrt{-2\ln(u_1)}\times\text{cos}(2\pi u_2)
z_1 <-\sqrt{-2\ln(u_1)}\times\text{cos}(2\pi u_2)
z_1 <- sqrt{-2\ln(u_1)}\times\text{cos}(2\pi u_2)
z-1 <- \sqrt(-2*\ln(u_1))*\cos(2*\pi*u_2)
z-1 <- sqrt(-2*\ln(u_1))*\cos(2*\pi*u_2)
z-1 <- \sqrt(-2*ln(u_1))*\cos(2*\pi*u_2)
z-1 <- sqrt(-2*ln(u_1))*\cos(2*\pi*u_2)
z_1 <- sqrt(-2*ln(u_1))*cos(2*pi*u_2)
z_1 <- sqrt(-2*log(u_1))*cos(2*pi*u_2)
z_2 <- sqrt(-2*log(u_1))*sin(2*pi*u_2)
z <- c(z_1, z_2)
mu <- 5
\mu <- 5
sigma <- 2
x <- mu + sigma * z
mean(x)
sd(x)
#| fig-cap: "Histogram of $x$"
#| dependson: q1
#| eval: false
#| echo: true
hist(x,
freq = FALSE,
ylab = "Density",
xlab = "$x$")
curve(dnorm(x, mean = mu, sd = sigma),
col = "red", lwd = 2, add = TRUE)
